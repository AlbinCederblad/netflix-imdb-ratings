{
  "name": "Netflix IMDB Ratings",
  "version": "1.0",
  "description": "Displays IMDB ratings of shows on Netflix",
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*", "https://www.omdbapi.com/*"],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "permissions": ["https://www.netflix.com/*", "https://www.omdbapi.com/*"],
  "manifest_version": 2
}
